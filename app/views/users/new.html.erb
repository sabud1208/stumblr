
    <h1> Create Profile!</h1>



    <!-- Session create form -->

    <% if flash[:error] %>
      <li><%= flash[:error] %></li>
    <% end %>


    <%= form_for @user do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :email %>
      <%= f.text_field :email %>
      <%= f.label :password %>
      <%= f.text_field :password %>
      <%= f.submit %>
    <% end %>
