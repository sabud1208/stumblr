<%#login %>
<%= form_tag "/sessions"  do %>
  <div class = "login"><label for="email">Email:</label></div>
  <%= text_field_tag :email %>
  <div class = "login"><label for="password">Password:</label></div>
  <%= text_field_tag :password %>
  <%= submit_tag "Login" %>
<% end %>

